description = XHTML Starter Theme for <a href="http://www.phase2technology.com/openpublic">Open Public</a> - Powered by <a href="http://himer.us/omega960">Omega</a>.
screenshot = screenshot.png
core       = 7.x
base theme = omega

; ------- Declare default stylesheets
stylesheets[all][] = css/open_omega.css

; Print-version styles
stylesheets[print][] = css/print.css

; IE 8 and below stylesheet
stylesheets-conditional[lt IE 8][all][] = css/openpublic_theme_ie8.css

; IE 7 and below stylesheet
stylesheets-conditional[lt IE 7][all][] = css/openpublic_theme_ie7.css

